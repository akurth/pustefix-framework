<?xml version="1.0" encoding="UTF-8"?><archetype-descriptor name="pustefix-archetype-application">
  <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.iwrp</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/pustefix</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/pustefix</directory>
      <includes>
        <include>**/*.xsl</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>.settings</directory>
      <includes>
        <include>**/*.prefs</include>
      </includes>
    </fileSet>
  </fileSets>
  <requiredProperties>
    <requiredProperty key="groupId">
      <defaultValue>com.foo</defaultValue>
    </requiredProperty>
    <requiredProperty key="artifactId">
      <defaultValue>com.foo</defaultValue>
    </requiredProperty>
    <requiredProperty key="package">
      <defaultValue>com.foo.bar</defaultValue>
    </requiredProperty>
  </requiredProperties>
</archetype-descriptor>