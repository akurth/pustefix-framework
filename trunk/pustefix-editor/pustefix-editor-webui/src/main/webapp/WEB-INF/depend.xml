<?xml version="1.0" encoding="UTF-8"?>
<make project="editor2" lang="en">
  
  <global>
    <param name="outputdoctype-public" value="-//W3C//DTD HTML 4.01//EN"/>
    <param name="outputdoctype-system" value="http://www.w3.org/TR/html40/strict.dtd"/>    
  </global>
    
  <standardmaster/>
  
  <standardmetatags>
    <include stylesheet="xsl/metatags.xsl"/>
  </standardmetatags>
  
  <standardpage name="login" xml="xml/two_frames_inactive.xml"/>
  <standardpage name="start" xml="xml/two_frames_inactive.xml"/>
  <standardpage name="home" xml="xml/two_frames.xml"/>
  <standardpage name="pages" xml="xml/three_frames.xml"/>
  <standardpage name="targets" xml="xml/three_frames.xml">
    <include stylesheet="xsl/runtime.xsl"/>
  </standardpage>
  <standardpage name="images" xml="xml/three_frames.xml"/>
  <standardpage name="includes" xml="xml/three_frames.xml">
    <include stylesheet="xsl/runtime.xsl"/>
  </standardpage>
  <standardpage name="dynincludes" xml="xml/three_frames.xml">
    <include stylesheet="xsl/runtime.xsl"/>
  </standardpage>
  <standardpage name="search" xml="xml/two_frames.xml"/>
  <standardpage name="sessionmanager" xml="xml/two_frames_inactive.xml"/>
  <standardpage name="useradmin" xml="xml/two_frames_inactive.xml"/>
  <standardpage name="useredit" xml="xml/two_frames_inactive.xml"/>
  
  <standardpage name="ws_includes" xml="xml/webservice.xml">
    <param name="outputmethod" value="xml"/>
    <include stylesheet="xsl/default_copy.xsl" module="pustefix-core"/>
  </standardpage>
  <standardpage name="ws_dynincludes" xml="xml/webservice.xml">
    <param name="outputmethod" value="xml"/>
    <include stylesheet="xsl/default_copy.xsl" module="pustefix-core"/>
  </standardpage>
</make>
