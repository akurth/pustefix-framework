<?xml version="1.0" encoding="UTF-8"?><include_parts xmlns:ixsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.schlund.de/pustefix/core">
  
  <part name="content">
    <theme name="default">
      <h1>Edit tasklist</h1>
      <table>
        <pfx:forminput>
          <pfx:xinp type="hidden" name="edit.id"/>
          <tr>
            <td>Name:</td>
            <td><pfx:xinp type="text" name="edit.name"/></td>
          </tr>
          <tr>
            <td>Description:</td>
            <td><pfx:xinp type="text" name="edit.description"/></td>
          </tr>
          <tr>
            <td colspan="2">
              <pfx:xinp type="submit" value="Submit"/>
            </td>
          </tr>
        </pfx:forminput>
      </table>
    </theme>
  </part>
  
</include_parts>
