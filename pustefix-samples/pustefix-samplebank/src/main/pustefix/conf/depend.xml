<?xml version="1.0" encoding="UTF-8"?>
<make project="samplebank"
      lang="en_GB">

  <global>
    <param name="outputdoctype-public" value="-//W3C//DTD HTML 4.01//EN"/>
    <param name="outputdoctype-system" value="http://www.w3.org/TR/html40/strict.dtd"/>
  </global>
  
  <navigation>
    <page name="home" handler="/xml/config"/>
    <page name="login"     handler="/xml/config"/>
    <page name="accountoverview"     handler="/xml/config" accesskey="H"/>
    <page name="accountdetails"    handler="/xml/config" accesskey="O"/>
    <page name="test"     handler="/xml/config"/>
    <page name="logout"     handler="/xml/config"/>
  </navigation>
  
  <standardmaster/>

  <standardmetatags>
    <include stylesheet="xsl/metatags.xsl"/>
  </standardmetatags>
  
  <standardpage name="home" xml="xml/one_frame.xml"/>
  <standardpage name="login" xml="xml/one_frame.xml"/>
  <standardpage name="accountoverview" xml="xml/one_frame.xml"/>
  <standardpage name="accountdetails" xml="xml/one_frame.xml"/>
  <standardpage name="test" xml="xml/one_frame.xml"/>

</make>
