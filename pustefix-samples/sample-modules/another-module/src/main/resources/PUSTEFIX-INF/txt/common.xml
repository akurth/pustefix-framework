<?xml version="1.0" encoding="utf-8"?>
<include_parts xmlns:ixsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.schlund.de/pustefix/core">
  <part name="foo">
    <theme name="default">
      <h1>fooooooooooooo</h1>
    </theme>
  </part>
  <part name="ext1">
    <extends extension-point="enterdata.content.top"/>
    <theme name="default">
      <p>Additional content from another module for top of content</p>
    </theme>
  </part>
<part name="app_test_another">
  <theme name="default">
    app_test_another (another)
  </theme>
</part>

<part name="cascade">
  <theme name="default">
     cascade (another)
     <pfx:include part="cascade_another"/>
  </theme>
</part>

<part name="cascade_another">
  <theme name="default">
    cascade_another (another)
    <pfx:include href="txt/foo.xml" part="foo"/>
  </theme>
</part>

 </include_parts>
