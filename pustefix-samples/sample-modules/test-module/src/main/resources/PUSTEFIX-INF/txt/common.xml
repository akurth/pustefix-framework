<?xml version="1.0" encoding="utf-8"?>
<include_parts xmlns:ixsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.schlund.de/pustefix/core">
  <part name="foo">
    <theme name="default">
      <h1>fooooooooooooo</h1>
    </theme>
  </part>
  <part name="ext1">
    <extends extension-point="enterdata.content.top"/>
    <theme name="default">
      <p>Additional content from test module for top of content</p>
    </theme>
  </part>
  <part name="ext2">
    <extends extension-point="common.body.top"/>
    <theme name="default">
      <p>Additional content from test module for top of page</p>
    </theme>
  </part>
  <part name="ext3">
    <extends extension-point="enterdata.error.testmisstheme"/>
    <theme name="zzz">
        zzzzzzzzzzzzzzzzzzz
    </theme>
  </part>

<part name="app_test">
  <theme name="default">
    app_test (test)
  </theme>
</part>

<part name="app_test_another">
  <theme name="default">
    app_test_another (test)
  </theme>
</part>

<part name="cascade">
  <theme name="default">
     cascade (test)
     <pfx:include part="cascade_test"/>
     <pfx:image src="img/tiger-small.jpg"/>
  </theme>
</part>

<part name="cascade_test">
  <theme name="default">
     cascade_test (test)
     <pfx:include href="txt/common.xml" part="cascade" module="org.pustefixframework.samples.modules.another-module"/>
  </theme>
</part>

 </include_parts>
