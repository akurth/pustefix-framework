<?xml version="1.0" encoding="utf-8"?>
<pustefix-module xmlns="http://www.pustefix-framework.org/2009/namespace/module-config">

  <application>
    
    <context-xml-service>
      <config-file>bundle:/META-INF/pustefix/app.conf.xml</config-file>
    </context-xml-service>
   
    <extension type="application.static">
       <extends extension-point="app.images"/>    
       <request-path-prefix>test</request-path-prefix>
       <path>img</path>
    </extension>
 
  </application>

  <xml-generator>
    <config-file>bundle:/META-INF/pustefix/depend.xml</config-file>
  </xml-generator>

  <dynamic-includes>
    <!--<auto-search level="2"/>-->
  </dynamic-includes>  

</pustefix-module>
