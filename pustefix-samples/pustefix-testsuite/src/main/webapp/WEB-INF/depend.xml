<?xml version="1.0" encoding="UTF-8"?>
<make project="testsuite" lang="en_US">
  
  <global>
    <param name="outputdoctype-public" value="-//W3C//DTD HTML 4.01//EN"/>
    <param name="outputdoctype-system" value="http://www.w3.org/TR/html40/strict.dtd"/>
  </global>
  
  <!-- prefix to namespace url mappings to be used in pages xml -->
  <namespaces>
    <namespace-declaration prefix="pfx" url="http://www.schlund.de/pustefix/core"/>
    <namespace-declaration prefix="ixsl" url="http://www.w3.org/1999/XSL/Transform"/>
  </namespaces>
  
  <standardmaster/>
  <standardmetatags/>
  
  <standardpage name="Start" xml="xml/main.xml"/>
  
  <standardpage name="Test1" xml="xml/test.xml"/>
  
  <standardpage name="Test2" xml="xml/test.xml"/>
  <standardpage name="Test2Failed" xml="xml/test.xml"/>
  <standardpage name="Test2Succeeded" xml="xml/test.xml"/>
  
  <standardpage name="Test3" xml="xml/test.xml"/>
  <standardpage name="Test3SetVariant" xml="xml/test.xml"/>
  <standardpage name="Test3Default" xml="xml/test.xml"/>
  <standardpage name="Test3VariantA" xml="xml/test.xml"/>
  
  <standardpage name="Test4" xml="xml/test.xml"/>
  <standardpage name="Test4Failed" xml="xml/test.xml"/>
  <standardpage name="Test4Succeeded" xml="xml/test.xml"/>
  
  <standardpage name="Test5" xml="xml/test.xml"/>
  <standardpage name="Test5Form" xml="xml/test.xml"/>
  <standardpage name="Test5Failed" xml="xml/test.xml"/>
</make>
