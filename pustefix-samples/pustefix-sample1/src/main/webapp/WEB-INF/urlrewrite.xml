<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" 
                            "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

  <rule>
    <from>^/xml/main/(.*)$</from>
    <to type="permanent-redirect">/$1?%{query-string}</to>
  </rule>

</urlrewrite>
