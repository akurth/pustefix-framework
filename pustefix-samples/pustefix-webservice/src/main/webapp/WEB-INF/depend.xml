<make cachedir=".cache/webservice"
      record_dir="recorded"
      project="webservice"
      lang="en">

  <global>
    <!-- <include stylesheet="xsl/runtime.xsl"/> -->
    <param   name="outputmethod"         value="html"/>
    <param   name="outputdoctype-public" value="-//W3C//DTD HTML 4.01//EN"/>
    <param   name="outputdoctype-system" value="http://www.w3.org/TR/html40/strict.dtd"/>
  </global>
  
  <!-- prefix to namespace url mappings to be used in pages xml -->
  <namespaces>
    <namespace-declaration prefix="pfx" url="http://www.schlund.de/pustefix/core"/>
    <namespace-declaration prefix="ixsl" url="http://www.w3.org/1999/XSL/Transform"/>
  </namespaces>
  
  <navigation>
    <page name="calculator"/>
    <page name="chat"/>
    <page name="counter"/>
    <page name="data"/>
    <page name="typetest"/>
    <page name="calltest"/>
    <page name="beantest"/>
  </navigation>

  <standardmaster>
    <!-- <include stylesheet="xsl/skin.xsl"/> -->
  </standardmaster>
  
  <standardmetatags>
    <include stylesheet="xsl/metatags.xsl"/>
  </standardmetatags>
  
  <standardpage name="calculator" xml="xml/two_frames.xml"/>
  <standardpage name="chat" xml="xml/two_frames.xml"/>
  <standardpage name="counter" xml="xml/two_frames.xml"/>
  <standardpage name="data" xml="xml/two_frames.xml"/>
  <standardpage name="typetest" xml="xml/two_frames.xml"/>
  <standardpage name="calltest" xml="xml/two_frames.xml"/>
  <standardpage name="beantest" xml="xml/two_frames.xml"/>

</make>
