<?xml version="1.0" encoding="utf-8"?>
<make lang="en" project="firstapp">

  <!-- prefix to namespace url mappings to be used in pages xml -->
  <namespaces>
    <namespace-declaration prefix="pfx" url="http://www.schlund.de/pustefix/core"/>
    <namespace-declaration prefix="ixsl" url="http://www.w3.org/1999/XSL/Transform"/>
  </namespaces>

  <global>
    <include stylesheet="xsl/runtime.xsl"/>
  </global>

  <standardmaster>
    <include stylesheet="xsl/skin.xsl"/>
  </standardmaster>

  <standardmetatags>
  	<include stylesheet="xsl/metatags.xsl"/>
  </standardmetatags>

  <auto-standardpage lookup-path="txt/pages" lookup-module="" xml="xml/frame.xml"/>

  <standardpage name="Info" xml="xml/frame.xml">
    <standardpage-alternative>cities</standardpage-alternative>
    <standardpage-alternative>nationalparks</standardpage-alternative>
    <standardpage-alternative>mountains</standardpage-alternative>
  </standardpage>

</make>