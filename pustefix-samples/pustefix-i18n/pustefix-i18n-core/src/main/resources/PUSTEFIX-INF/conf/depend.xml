<?xml version="1.0" encoding="utf-8"?>
<make lang="en" project="firstapp">

  <!-- prefix to namespace url mappings to be used in pages xml -->
  <namespaces>
    <namespace-declaration prefix="pfx" url="http://www.schlund.de/pustefix/core"/>
    <namespace-declaration prefix="ixsl" url="http://www.w3.org/1999/XSL/Transform"/>
  </namespaces>

  <global>
    <include stylesheet="xsl/runtime.xsl"/>
  </global>

  <standardmaster>
    <include stylesheet="xsl/skin.xsl"/>
  </standardmaster>

  <standardmetatags>
  	<include stylesheet="xsl/metatags.xsl"/>
  </standardmetatags>
  
  <standardpage-variant tenant="US_market" lang="en_US">US_market-en_US</standardpage-variant>
  <standardpage-variant tenant="CA_market" lang="en_CA">CA_market-en_CA</standardpage-variant>
  <standardpage-variant tenant="CA_market" lang="fr_CA">CA_market-fr_CA</standardpage-variant>
  <standardpage-variant tenant="DE_market" lang="de_DE">DE_market-de_DE</standardpage-variant>

  <auto-standardpage lookup-path="txt/pages" lookup-module="" xml="xml/frame.xml"/>

  <standardpage name="Info" xml="xml/frame.xml">
    <standardpage-variant>cities</standardpage-variant>
    <standardpage-variant>nationalparks</standardpage-variant>
    <standardpage-variant>mountains</standardpage-variant>
  </standardpage>

</make>