<?xml version="1.0" encoding="ISO-8859-1"?>
<properties xmlns:cus="http://www.schlund.de/pustefix/customize">

  <prop name="pustefix.log4j.config"><cus:docroot/>common/conf/pfixlog.xml</prop>

  <!-- List of all factories to init -->
  <prop name="factory.initialize.00">de.schlund.pfixxml.loader.AppLoader</prop>
  <prop name="factory.initialize.01">de.schlund.util.statuscodes.StatusCodeFactory</prop>
  <prop name="factory.initialize.02">de.schlund.pfixxml.targets.SPCacheFactory</prop>
  <prop name="factory.initialize.03">de.schlund.pfixcore.editor.auth.AuthManagerFactory</prop>
  <prop name="factory.initialize.04">de.schlund.pfixcore.editor.EditorProductFactory</prop>
  <prop name="factory.initialize.05">de.schlund.pfixcore.editor.EditorPageUpdater</prop>
  <prop name="factory.initialize.06">de.schlund.pfixcore.editor.EditorCommonsFactory</prop>
  <prop name="factory.initialize.07">de.schlund.pfixxml.exceptionhandler.ExceptionHandler</prop>

  <!-- StatusCodeFactory -->
  <prop name="statuscodefactory.propertyfile.1">de/schlund/pfixcore/generator/StatusCodes.prop</prop>
  <prop name="statuscodefactory.propertyfile.2">de/schlund/pfixcore/example/StatusCodes.prop</prop>
  <prop name="statuscodefactory.propertyfile.3">de/schlund/pfixcore/editor/StatusCodes.prop</prop>

  <!-- SPCacheFactory -->
  <prop name="targetcache.cacheclass">de.schlund.pfixxml.targets.LRUCache</prop>
  <prop name="targetcache.cachecapacity">50</prop>
  <prop name="includecache.cacheclass">de.schlund.pfixxml.targets.LRUCache</prop>
  <prop name="includecache.cachecapacity">50</prop>

  <!-- EditorUserFactory -->
  <prop name="editoruserfactory.userdata"><cus:docroot/>common/conf/userdata.xml</prop>

  <!-- EditorProductFactory -->
  <cus:choose>
    <cus:test mode="prod">
      <prop name="editorproductfactory.delayinit">false</prop>
    </cus:test>
  </cus:choose>
  <prop name="editorproductfactory.productdata"><cus:docroot/>common/conf/products.prj</prop>

  <!-- EditorCommonsFactory -->
  <prop name="editorcommonsfactory.includefile.1"><cus:docroot/>common/dyntxt/statusmessages.xml</prop>
  
  <!--ExceptionHandler-->
  <prop name="exceptionhandler.propertyfile"><cus:docroot/>common/conf/exceptionhandler.prop</prop>
  
  <!--AppLoader-->
  <prop name="apploader.propertyfile"><cus:docroot/>common/conf/apploader.prop</prop>
  
</properties>

<!--
Local Variables:
mode: xml
End:
-->
