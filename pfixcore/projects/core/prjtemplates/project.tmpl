  <active>true</active>
  <comment>Your comment will be added here</comment>
  <depend>myproject/conf/depend.xml</depend>
  <virtualhost><cus:fqdn/></virtualhost>
  <servername>myproject.<cus:fqdn/></servername>
  <serveralias>myproject.<cus:machine/></serveralias>
  
  <defpath>/xml/config</defpath>
  <passthrough>myproject/img</passthrough>
  
  <documentroot><cus:docroot/>myproject/htdocs</documentroot>
  
  <literalapache>
    # Add something here...
  </literalapache>
  <ports>
    <port number="80"/>
  </ports>
  
  <servlet name="deref">
    <active>true</active>
    <class>de.schlund.pfixxml.DerefServlet</class>
  </servlet>

</project>

