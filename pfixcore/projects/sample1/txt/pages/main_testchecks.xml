          <pfx:error><pfx:scode/></pfx:error>
        </pfx:checkfield>
        <br/>
        
        File: <input type="file" name="checks.File" />
        <pfx:checkfield name="checks.File">
          <pfx:error><pfx:scode/></pfx:error>
        </pfx:checkfield>
        <br/>
        
        UploadFile: <input type="file" name="checks.UploadFile" />
        <pfx:checkfield name="checks.UploadFile">
          <pfx:error><pfx:scode/></pfx:error>
        </pfx:checkfield>
        <br/>
        
        <br/><br/>
        <pfx:xinp type="submit" value="submit"/>
