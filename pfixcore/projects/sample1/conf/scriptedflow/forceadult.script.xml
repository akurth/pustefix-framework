<?xml version="1.0" encoding="ISO-8859-1"?>
<scriptedflow version="1.0" xmlns="http://pustefix.sourceforge.net/scriptedflow200602" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pustefix.sourceforge.net/scriptedflow200602 http://pustefix.sourceforge.net/scriptedflow200602.xsd">
  <!-- 
       Simple example that skips the "Are you old enough?" question,
       automatically answering it with "yes" and proceeds to the order page.
       Open 
         http://sample1.<yourhost>/xml/order/?__scriptedflow=forceadult
       in your browser to see a demonstration.
  -->
  <virtual-request page="home">
    <param name="__sendingdata">1</param>
    <param name="info.Adult">true</param>
  </virtual-request>
</scriptedflow>