<?xml version="1.0" encoding="ISO-8859-1"?>
<include_parts xmlns:ixsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.schlund.de/pustefix/core">
  <part name="success">
    <theme name="default">
      <div style="margin-left:auto;margin-right:auto;text-align:center;border:1px solid #000000;background-color:#e0ffe0">
        The test was <br/>
        <span style="font-weight:bold;color:#00b000">SUCCESSFUL</span>
      </div>
    </theme>
  </part>
  <part name="failure">
    <theme name="default">
      <div style="margin-left:auto;margin-right:auto;text-align:center;border:1px solid #000000;background-color:#ffe0e0">
        The test <br/>
        <span style="font-weight:bold;color:#ff0000">FAILED</span>
      </div>
    </theme>
  </part>
</include_parts>