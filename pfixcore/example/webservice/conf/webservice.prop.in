<properties xmlns:cus="http://www.schlund.de/pustefix/customize">

	<webservice-global>
		<requestpath>/xml/webservice</requestpath>
		<wsdlsupport enabled="true" repository="/wsdl"/>
		<monitoring enabled="true" scope="session"/>
		<logging enabled="true"/>
	</webservice-global>
	
	<webservice name="Calculator">
		<interface name="de.schlund.pfixcore.example.webservices.Calculator"/>
		<implementation name="de.schlund.pfixcore.example.webservices.CalculatorImpl"/>
		<param name="p1" value="7"/>
		<param name="p2" value="3"/>
		<session type="none"/>
	</webservice>
	
	<webservice name="Counter">
		<interface name="de.schlund.pfixcore.example.webservices.Counter"/>
		<implementation name="de.schlund.pfixcore.example.webservices.CounterImpl"/>
		<context name="pfixcore_project:webservice::servlet:config"/>
		<session type="servlet"/>
	</webservice>
	
	<webservice name="Data">
		<interface name="de.schlund.pfixcore.example.webservices.Data"/>
		<implementation name="de.schlund.pfixcore.example.webservices.DataImpl"/>
		<context name="pfixcore_project:webservice::servlet:config"/>
		<session type="servlet"/>
	</webservice>
	
  
</properties>

<!--
Local Variables:
mode: xml
End:
-->

