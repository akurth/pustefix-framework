  <servletinfo depend="sample1/conf/depend.xml" name="pfixcore_project:sample1::servlet:directoutput">
    <ssl force="true"/>
    <editmode allow="true"/>
  </servletinfo>
  
  <foreigncontext externalservletname="pfixcore_project:sample1::servlet:config"/>
  
  <directoutputpagerequest name="install">
    <directoutputstate class="de.schlund.pfixcore.workflow.app.FileDownloadDOState"/>
    <param name="downloadfile">INSTALL.ps</param>
    <param name="downloadmimetype">text/ps</param>
  </directoutputpagerequest>
  
  
</properties>

