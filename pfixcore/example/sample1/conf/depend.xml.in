<make project="sample1"
      lang="en_GB">

  <global>
    <!-- <include stylesheet="sample1/xsl/runtime.xsl"/> -->
    <!-- <param   name="outputmethod"         value="html"/> -->
    <param name="outputdoctype-public" value="-//W3C//DTD HTML 4.01//EN"/>
    <param name="outputdoctype-system" value="http://www.w3.org/TR/html40/strict.dtd"/>
  </global>
  
  <navigation>
    <page name="home"     handler="/xml/config" accesskey="H">
      <page name="order"    handler="/xml/config" accesskey="O"/>
      <page name="overview" handler="/xml/config" accesskey="S"/>
    </page>
    
    <page name="admin"    handler="/xml/config"/>
    <page name="test"     handler="/xml/config"/>
    <page name="encoding" handler="/xml/config"/>

    <page name="ssltest"  handler="/xml/secure"/>
    <page name="install"  handler="/xml/download"/>
  </navigation>

  <standardmaster>
    <!-- <include stylesheet="sample1/xsl/skin.xsl"/> -->
  </standardmaster>
  
  <standardmetatags>
    <include stylesheet="sample1/xsl/metatags.xsl"/>
  </standardmetatags>

  <standardpage name="home"     xml="sample1/xml/two_frames.xml"/>
  <standardpage name="home"     xml="sample1/xml/two_frames.xml" variant="foo"/>
  <standardpage name="home"     xml="sample1/xml/two_frames.xml" variant="foo:bar:baz"/>
  <standardpage name="home"     xml="sample1/xml/two_frames.xml" variant="baz"/>
  <standardpage name="order"    xml="sample1/xml/two_frames.xml"/>
  <standardpage name="overview" xml="sample1/xml/two_frames.xml"/>

  <standardpage name="admin"    xml="sample1/xml/one_frame.xml"/>
  <standardpage name="test"     xml="sample1/xml/one_frame.xml"/>

  <standardpage name="ssltest"  xml="sample1/xml/one_frame.xml"/>
  <standardpage name="encoding" xml="sample1/xml/one_frame.xml"/>

</make>
