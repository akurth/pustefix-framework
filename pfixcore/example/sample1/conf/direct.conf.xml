<?xml version="1.0" encoding="ISO-8859-1"?>
<directoutputserver xmlns="http://pustefix.sourceforge.net/properties200401" xmlns:cus="http://www.schlund.de/pustefix/customize" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://pustefix.sourceforge.net/properties200401 http://pustefix.sourceforge.net/properties200401.xsd" version="1.0">
  
  <directoutputservletinfo depend="sample1/conf/depend.xml" name="pfixcore_project:sample1::servlet:directoutput">
    <editmode allow="true"/>
    <ssl force="true"/>
  </directoutputservletinfo>
  
  <foreigncontext externalservletname="pfixcore_project:sample1::servlet:config"/>
  
  <directoutputpagerequest name="install">
    <directoutputstate class="de.schlund.pfixcore.workflow.app.FileDownloadDOState"/>
    <properties>
      <prop name="downloadfile">README.1st</prop>
      <prop name="downloadmimetype">text/plain</prop>
    </properties>
  </directoutputpagerequest>
  
</directoutputserver>

