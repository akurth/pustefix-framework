<?xml version="1.0" encoding="ISO-8859-1"?>
<include_parts xmlns:pfx="http://www.schlund.de/pustefix/core"
               xmlns:ixsl="http://www.w3.org/1999/XSL/Transform">
  
  <part name="logoutbutton">
    <product name="default">
      <lang name="default">
        <pfx:button target="_top" mode="force" page="prodselect">Cancel</pfx:button>
      </lang>
    </product>
  </part>

  <part name="content">
    <product name="default">
      <lang name="default">
        <center>
          <br/><br/>
          <table width="90%">
            <tr>
              <td>
                <span style="color: #ffffff; font-weight: bold;">Currently registered locks:</span>
              </td>
            </tr>
            <tr>
              <td align="center">
                <displayalllocks/>
              </td>
            </tr>
          </table>
        </center>
      </lang>
    </product>
  </part>
  
</include_parts>

