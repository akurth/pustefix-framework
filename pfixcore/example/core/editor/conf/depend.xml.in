<!-- -*- mode: xml -*- -->
<make cachedir=".cache/editor" record_allowed="false" record_dir="editor/recorded"
      xmlns:cus="http://www.schlund.de/pustefix/customize">
  
  <navigation>
    <page name="editoruserauth"    handler="/xml/edit"/>
    <page name="prodselect"        handler="/xml/edit"/>
    <page name="editoruserdata"    handler="/xml/edit"/>
    <page name="sessionmanager"    handler="/xml/edit"/>
    <page name="lockmanager"       handler="/xml/edit"/>
    <page name="editorusermanager" handler="/xml/edit"/>
    <page name="home"              handler="/xml/edit"/>
    <page name="pages"             handler="/xml/edit"/>
    <page name="images"            handler="/xml/edit"/>
    <page name="includes"          handler="/xml/edit"/>
    <page name="commons"           handler="/xml/edit"/>
    <page name="targets"           handler="/xml/edit"/>
    <page name="search"            handler="/xml/edit"/>
    <page name="logout"            handler="/xml/edit"/>
    <page name="documentation"     handler="/xml/edit"/>
    <page name="testcaseselect"    handler="/xml/edit"/>
    <page name="testcasestart"     handler="/xml/edit"/>
    <page name="testcaseoverview"  handler="/xml/edit"/>
  </navigation>

  <!-- ========= master =============== -->
  <target name="master.xsl" type="xsl">
    <depxml name="core/xsl/master.xsl"/>
    <depxsl name="core/editor/xsl/custommaster.xsl"/>
    <depaux name="core/xsl/default_copy.xsl"/>
    <depaux name="core/xsl/include.xsl"/>
    <depaux name="core/xsl/navigation.xsl"/>
    <depaux name="core/xsl/utils.xsl"/>
    <depaux name="core/xsl/forminput.xsl"/>
    <param name="product" value="editor"/>
    <param name="lang"    value="en"/>
  </target>

  <!-- ========= metatags =============== -->
  <target name="metatags.xsl" type="xsl">
    <depxml name="core/editor/xsl/metatags.xsl"/>
    <depxsl name="core/xsl/customizestylesheet.xsl"/>
    <depaux name="core/xsl/include.xsl"/>
    <depaux name="core/xsl/default_copy.xsl"/>
    <depaux name="core/xsl/utils.xsl"/>
    <param name="product" value="editor"/>
    <param name="lang"    value="en"/>
  </target>

  <standardpage name="editoruserauth"    xml="core/editor/xml/two_frames_deaktiv.xml"/>
  <standardpage name="prodselect"        xml="core/editor/xml/two_frames_deaktiv.xml"/>
  <standardpage name="lockmanager"       xml="core/editor/xml/two_frames_deaktiv.xml"/>
  <standardpage name="sessionmanager"    xml="core/editor/xml/two_frames_deaktiv.xml"/>
  <standardpage name="editoruserdata"    xml="core/editor/xml/two_frames_deaktiv.xml"/>
  <standardpage name="editorusermanager" xml="core/editor/xml/two_frames_deaktiv.xml"/>
  <standardpage name="home"              xml="core/editor/xml/two_frames.xml"/>
  <standardpage name="pages"             xml="core/editor/xml/three_frames_wide.xml"/>
  <standardpage name="targets"           xml="core/editor/xml/three_frames_wide.xml"/>
  <standardpage name="images"            xml="core/editor/xml/three_frames.xml"/>
  <standardpage name="includes"          xml="core/editor/xml/three_frames_applet.xml"/>
  <standardpage name="commons"           xml="core/editor/xml/three_frames_applet.xml"/>
  <standardpage name="search"            xml="core/editor/xml/two_frames.xml"/>
  <standardpage name="documentation"     xml="core/editor/xml/three_frames_doku.xml"/>
  <standardpage name="testcaseselect"    xml="core/editor/xml/two_frames.xml"/>
  <standardpage name="testcasestart"     xml="core/editor/xml/two_frames.xml"/>
  <standardpage name="testcaseoverview"     xml="core/editor/xml/two_frames.xml"/>
  <!-- These pages should never be visible -->
  <standardpage name="logout"            xml="core/editor/xml/empty.xml"/>

</make>
