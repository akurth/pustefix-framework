<?xml version="1.0"?>
<configuration
  xmlns="http://www.marsching.com/2008/flexiparse/configurationNS"
  xmlns:c="http://www.pustefix-framework.org/2008/namespace/properties-config"
>
  
  <handler class="org.pustefixframework.config.generic.PropertyParsingHandler">
    <match>/c:properties//c:prop</match>
    <match>/c:properties//c:property</match>
  </handler> 
  
  <handler id="org.pustefixframework.config.project.customization.WhenParsingHandler" class="org.pustefixframework.config.customization.CustomizationWhenParsingHandler">
    <match>//c:choose/c:when</match>
  </handler>
  
  <handler id="org.pustefixframework.config.project.customization.OtherwiseParsingHandler" class="org.pustefixframework.config.customization.CustomizationOtherwiseParsingHandler">
    <match>//c:choose/c:otherwise</match>
  </handler>
  
</configuration>
