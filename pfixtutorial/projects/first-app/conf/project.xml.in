<?xml version="1.0" encoding="utf-8"?><project xmlns:cus="http://www.schlund.de/pustefix/customize" name="first-app">

<!--
  This page is included by the servletconf/projects.xml.in  
  "<insertexternalprojects/>" tag. 
  The basic project settings are made in this file. Never edit the
  project.xml because it is generated by the project.xml.in. 
  For more informations take a look here:
  	
  http://pustefix.oss.schlund.de/index.php?pagename=ProjectDefinition
-->  

  <active>true</active>
  <comment>My first Pustefix application</comment>
  <depend>first-app/conf/depend.xml</depend>
  <virtualhost><cus:fqdn/></virtualhost>
  <servername>first-app.<cus:fqdn/></servername>
  <serveralias>first-app.<cus:machine/></serveralias>
  
  <defpath>/xml/app</defpath>
  <passthrough>first-app/img</passthrough>
  
  <documentroot><cus:docroot/>first-app/htdocs</documentroot>
  
  <literalapache>
    # Add something here...
  </literalapache>
  <ports>
    <port number="80"/>
  </ports>
  
  <servlet name="app" useineditor="true">
    <active>true</active>
    <class>de.schlund.pfixxml.ContextXMLServlet</class>
    <propfile>first-app/conf/app.conf.xml</propfile>
  </servlet>
  <servlet name="deref">
    <active>true</active>
    <class>de.schlund.pfixxml.DerefServlet</class>
  </servlet>

</project>