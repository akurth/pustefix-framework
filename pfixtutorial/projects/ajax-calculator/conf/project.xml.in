<?xml version="1.0" encoding="utf-8"?><project xmlns:cus="http://www.schlund.de/pustefix/customize" name="ajax-calculator">

<!--
  This page is included by the servletconf/projects.xml.in  
  "<insertexternalprojects/>" tag. 
  The basic project settings are made in this file. Never edit the
  project.xml because it is generated by the project.xml.in. 
  For more informations take a look here:
  	
  http://pustefix.oss.schlund.de/index.php?pagename=ProjectDefinition
-->  

  <active>true</active>
  <comment>AJAX calculator tutorial</comment>
  <depend>ajax-calculator/conf/depend.xml</depend>
  <virtualhost><cus:fqdn/></virtualhost>
  <servername>ajax-calculator.<cus:fqdn/></servername>
  <serveralias>ajax-calculator.<cus:machine/></serveralias>
  
  <defpath>/xml/calc</defpath>
  <passthrough>ajax-calculator/img</passthrough>
  
  <documentroot><cus:docroot/>ajax-calculator/htdocs</documentroot>
  
  <literalapache>
    # Add something here...
  </literalapache>
  <ports>
    <port number="80"/>
  </ports>
  
  <servlet name="calc" useineditor="true">
    <active>true</active>
    <class>de.schlund.pfixxml.ContextXMLServlet</class>
    <propfile>ajax-calculator/conf/calc.conf.xml</propfile>
  </servlet>
  <servlet name="deref">
    <active>true</active>
    <class>de.schlund.pfixxml.DerefServlet</class>
  </servlet>

</project>