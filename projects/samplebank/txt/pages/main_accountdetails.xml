<?xml version="1.0" encoding="ISO-8859-1"?><include_parts xmlns:ixsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.schlund.de/pustefix/core">
  
  <part name="content">
    <theme name="default">
      <h1>Account details:</h1>
      <table>
        <tr>
          <th align="left">Account no.:</th>
          <td><ixsl:value-of select="/formresult/details/account/@accountNo"/></td>
        </tr>
        <tr>
          <th align="left">Opening date:</th>
          <td><ixsl:value-of select="/formresult/details/account/@openingDate"/></td>
        </tr>
        <tr>
          <th align="left">Debit:</th>
          <td><ixsl:value-of select="/formresult/details/account/@debit"/>&#160; <ixsl:value-of select="/formresult/details/account/@currency"/></td>
        </tr>
      </table>
      <h2>Last transactions:</h2>
      None
    </theme>
  </part>
  
</include_parts>
