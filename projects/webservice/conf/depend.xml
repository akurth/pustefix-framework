<make cachedir=".cache/webservice"
      record_dir="webservice/recorded"
      project="webservice"
      lang="en">

  <global>
    <!-- <include stylesheet="webservice/xsl/runtime.xsl"/> -->
    <param   name="outputmethod"         value="html"/>
    <param   name="outputencoding"       value="iso-8859-1"/>
    <param   name="outputdoctype-public" value="-//W3C//DTD HTML 4.01//EN"/>
    <param   name="outputdoctype-system" value="http://www.w3.org/TR/html40/strict.dtd"/>
  </global>
  
  <navigation>
    <page name="calculator" handler="/xml/config"/>
    <page name="chat" handler="/xml/config"/>
    <page name="counter" handler="/xml/config"/>
    <page name="data" handler="/xml/config"/>
    <page name="typetest" handler="/xml/config"/>
    <page name="calltest" handler="/xml/config"/>
    <page name="beantest" handler="/xml/config"/>
  </navigation>

  <standardmaster>
    <!-- <include stylesheet="webservice/xsl/skin.xsl"/> -->
  </standardmaster>
  
  <standardmetatags>
    <include stylesheet="webservice/xsl/metatags.xsl"/>
  </standardmetatags>
  
  <standardpage name="calculator" xml="webservice/xml/two_frames.xml"/>
  <standardpage name="chat" xml="webservice/xml/two_frames.xml"/>
  <standardpage name="counter" xml="webservice/xml/two_frames.xml"/>
  <standardpage name="data" xml="webservice/xml/two_frames.xml"/>
  <standardpage name="typetest" xml="webservice/xml/two_frames.xml"/>
  <standardpage name="calltest" xml="webservice/xml/two_frames.xml"/>
  <standardpage name="beantest" xml="webservice/xml/two_frames.xml"/>

</make>
