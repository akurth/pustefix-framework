<?xml version="1.0"?>
<configuration
  xmlns="http://www.marsching.com/2008/flexiparse/configurationNS"
  xmlns:c="http://www.pustefix-framework.org/2008/namespace/direct-output-service-config"
>
  
  <handler class="org.pustefixframework.config.directoutputservice.parser.DirectOutputPageRequestConfigExtensionParsingHandler" run-order="end">
    <match>/c:direct-output-service-config/c:extension[@type='direct.pagerequest']</match>
  </handler>
  
  <handler class="org.pustefixframework.config.directoutputservice.parser.DirectOutputPageRequestParsingHandler" run-order="both">
    <match>/c:direct-output-service-config/c:extension[@type='direct.pagerequest']/c:directoutputpagerequest</match>
  </handler>
  
  <handler class="org.pustefixframework.config.directoutputservice.parser.DirectOutputPageRequestConfigExtensionPointParsingHandler">
    <match>/c:direct-output-service-config/c:extension[@type='direct.pagerequest']/c:extension-point[@type='direct.pagerequest']</match>
  </handler>
  
  <handler class="org.pustefixframework.config.generic.PropertiesParsingHandler">
    <match>/c:direct-output-service-config/c:extension[@type='direct.pagerequest']/c:directoutputpagerequest/c:properties</match>
  </handler>
  
  <handler class="org.pustefixframework.config.generic.PropertyParsingHandler">
    <match>/c:direct-output-service-config/c:extension[@type='direct.pagerequest']/c:directoutputpagerequest/c:properties//c:prop</match>
  </handler>
  
  
  <handler id="org.pustefixframework.config.customization.WhenParsingHandler" class="org.pustefixframework.config.customization.CustomizationWhenParsingHandler">
    <match>//c:choose/c:when</match>
  </handler>
  
  <handler id="org.pustefixframework.config.customization.OtherwiseParsingHandler" class="org.pustefixframework.config.customization.CustomizationOtherwiseParsingHandler">
    <match>//c:choose/c:otherwise</match>
  </handler>
  
</configuration>
