<?xml version="1.0" encoding="ISO-8859-1"?>
<properties xmlns:cus="http://www.schlund.de/pustefix/customize">
  <cus:choose>
    <cus:test mode="prod">
      <prop name="rule.mail.send">true</prop>
      <prop name="rule.mail.to"><cus:uid/>@<cus:fqdn/></prop>
    </cus:test>
    <cus:test>
      <prop name="rule.mail.send">false</prop>
    </cus:test>
  </cus:choose>
  
  <prop name="rule.mail.from">PfixXptn@<cus:fqdn/></prop>
  <prop name="rule.mail.host">localhost</prop>

  <prop name="rule.general.use_me">true</prop>
  <prop name="rule.general.cleanuptask_schedule">10</prop>
  <prop name="rule.general.cleanuptask_dim">hour</prop>
  <prop name="rule.general.stracech_obsolete">2</prop>
  <prop name="rule.general.stracech_obsolete_dim">hour</prop>
  <prop name="rule.general.reporttask_schedule">2</prop>
  <prop name="rule.general.reporttask_dim">hour</prop>

  <prop name="rule.1.type">de.schlund.pfixxml.XMLException</prop>
  <prop name="rule.1.match">strace</prop>
  <prop name="rule.1.limit">1</prop>
  <prop name="rule.1.dim">min</prop>
  <prop name="rule.1.burst">2</prop>

  <prop name="rule.2.type">java.sql.SQLException</prop>
  <prop name="rule.2.match">type</prop>
  <prop name="rule.2.limit">1</prop>
  <prop name="rule.2.dim">hour</prop>
  <prop name="rule.2.burst">5</prop>

  <prop name="rule.3.type">java.lang.NullPointerException</prop>
  <prop name="rule.3.match">strace</prop>
  <prop name="rule.3.limit">3</prop>
  <prop name="rule.3.dim">hour</prop>
  <prop name="rule.3.burst">2</prop>
</properties>

