<?xml version="1.0" encoding="UTF-8"?>
<configuration
  xmlns="http://www.marsching.com/2008/flexiparse/configurationNS"
  xmlns:p="http://www.pustefix-framework.org/2009/namespace/application-config">

  <handler class="org.pustefixframework.webservices.spring.WebServiceRequestHandlerParsingHandler">
    <match>/p:pustefix-application/p:application/p:webservice-service</match>
    <match>/p:pustefix-application/p:application/p:webservice-service//p:admin</match>
    <match>/p:pustefix-application/p:application/p:webservice-service//p:monitoring</match>
    <match>/p:pustefix-application/p:application/p:webservice-service//p:logging</match>
  </handler>

  <handler class="org.pustefixframework.config.customization.CustomizationWhenParsingHandler">
    <match>/p:pustefix-application/p:application/p:webservice-service/p:choose/p:when</match>
  </handler>
  
  <handler class="org.pustefixframework.config.customization.CustomizationOtherwiseParsingHandler">
    <match>/p:pustefix-application/p:application/p:webservice-service/p:choose/p:otherwise</match>
  </handler>

</configuration>
  